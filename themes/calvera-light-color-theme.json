	"tokenColors": [
		{
			"name": "CSS Property Value",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#3339",
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#5a5a8d"  // Slightly darker for better readability on light background
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#353535"  // Darker tone for contrast
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function",
				"meta.function-call"
			],
			"settings": {
				"foreground": "#3f3fbc"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#d53c5f"  // More contrast while keeping the red tone
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#2953da"  // Lower luminance for better visibility in light mode
			}
		},
		{
			"name": "Operator Symbols",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#3b3bcc"  // Darkened for more contrast
			}
		},
		{
			"name": "String",
			"scope": [
				"string",  // Matches all string literals
				"string.quoted",  // Specifically for quoted strings
				"string.quoted.single",  // For single-quoted strings like 'hi i am string'
				"string.quoted.double"  // For double-quoted strings like "hi i am string"
			],
			"settings": {
				"foreground": "#2b7b77"  // Set your desired color for strings here
			}
		},
		{
			"name": "CSS ID",
			"scope": [
				"entity.other.attribute-name.id",
				"entity.other.attribute-name.pseudo-class"
			],
			"settings": {
				"foreground": "#6d1ced",
			}
		},
		{
				"name": "Class, Interface, Enum",
				"scope": [
						"entity.name.class",
						"type",
						"entity.name.type",
						"entity.name.type.alias",
						"entity.name.type.class",
						"entity.name.type.interface",
						"entity.name.type.enum",
						"entity.other.attribute-name.class",
						"constant.numeric",
						"constant.language.boolean",
						"keyword.other.unit",
						"meta.property-value",
						"meta.function.type",
						"entity.name.function.type",
						"variable.parameter.type",
						"variable.other.type",
						"variable.other.type.parameter",
						"entity.name.type.parameter",
						"support.type.primitive"
				],
				"settings": {
						"foreground": "#c77d1f"
				}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#eb4c4c"  // Slightly darker for enhanced contrast
			}
		},
		{
